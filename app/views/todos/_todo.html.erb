<div id="<%= dom_id todo %>">
  <div>
    <strong>Name:</strong>
    <%= todo.name %>
  </div>

  <div>
    <strong>Description:</strong>
    <%= todo.description %>
  </div>
   <div>
    <strong>Completed:</strong>
    <%= todo.completed %>
  </div>
   <div>
    <strong>Priority:</strong>
    <%= todo.priority %>
  </div>
   <div>
    <strong>Project:</strong>
    <%= link_to todo.project.name, project_path(todo.project)%>
  </div>

</div>
